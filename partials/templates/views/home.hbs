  <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img src="https://source.unsplash.com/500x900/?water" class="d-block w-100" alt="pic1" style="overflow: hidden;">
    </div>
    <div class="carousel-item">
      <img src="https://source.unsplash.com/500x900/?sunrise" class="d-block w-100" alt="pic2" style="overflow: hidden;">
